{
  "name": "inceptum-cqrs",
  "version": "0.2.0",
  "description": "Extension to Inceptum to build apps based on CQRS",
  "main": "src/index.js",
  "scripts": {
    "test": "NODE_ENV=test nyc mocha",
    "itest": "NODE_ENV=test nyc mocha itest",
    "lint": "tslint -p . --type-check",
    "docs": "jsdoc -c jsdocconf.json -t ./node_modules/ink-docstrap/template -R README.md -r -d docs/api",
    "precommit": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hipages/inceptum-cqrs.git"
  },
  "engine": {
    "node": ">6.0.0"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "inceptum": "0.2.0",
    "jsonwebtoken": "7.3.0",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "7.2.0",
    "co-mocha": "^1.1.2",
    "doubleagent": "^1.1.0",
    "husky": "^0.11.6",
    "ink-docstrap": "1.3.0",
    "istanbul": "0.4.4",
    "jsdoc": "3.4.3",
    "mocha": "^2.5.3",
    "mocha-typescript": "^1.1.4",
    "must": "^0.13.4",
    "normalize-path": "^2.1.1",
    "nyc": "^11.0.2",
    "sinon": "^1.17.7",
    "ts-node": "^3.0.4",
    "tslint": "^5.4.3",
    "tslint-config-shopify": "^2.0.0",
    "typescript": "^2.3.2"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
